
 <%= form_with url: new_restaurant_path, local: true do |f| %>
  <div class="field">
    <%= f.label :name %><br/>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>

   <%= f.hidden_field :user_id %>
  
   <%= f.submit 'Опубликовать' %>
 <% end %>
 

